       tabla_estudiantes.apellido_paterno,
	   tabla_estudiantes.apellido_materno,
	   tabla_estudiantes.nombres,
	   count(CASE WHEN tabla_historial_asistencias.asistencia = true Then 1 END) AS asistencia,
	   count(CASE WHEN tabla_historial_asistencias.asistencia = false Then 1 END) AS inasistencia
FROM tabla_historial_asistencias
LEFT JOIN tabla_estudiantes 
ON tabla_historial_asistencias.id_estudiante = tabla_estudiantes.id_estudiante
GROUP BY codigo_matricula, asistencia
ORDER BY tabla_estudiantes.codigo_matricula


SELECT 
    tabla_estudiantes.codigo_matricula,
    tabla_estudiantes.apellido_paterno,
    tabla_estudiantes.apellido_materno,
    tabla_estudiantes.nombres,
	max(CASE WHEN tabla_historial_asistencias.fecha_asistencia = '2024-07-30' THEN tabla_historial_asistencias.asistencia ELSE 0 END) AS '2024-07-30',
    max(CASE WHEN tabla_historial_asistencias.fecha_asistencia = '2024-08-10' THEN tabla_historial_asistencias.asistencia ELSE 0 END) AS '2024-08-10',
    max(CASE WHEN tabla_historial_asistencias.fecha_asistencia = '2024-08-12' THEN tabla_historial_asistencias.asistencia ELSE 0 END) AS '2024-08-12',
	count(CASE WHEN tabla_historial_asistencias.asistencia = true Then 1 END) AS asistencia,
	count(CASE WHEN tabla_historial_asistencias.asistencia = false Then 1 END) AS inasistencia
FROM tabla_estudiantes
LEFT JOIN tabla_historial_asistencias 
ON tabla_estudiantes.id_estudiante = tabla_historial_asistencias.id_estudiante
GROUP BY 
	tabla_estudiantes.codigo_matricula
ORDER BY 
    tabla_estudiantes.codigo_matricula;

SELECT * 
FROM tabla_estudiantes
LEFT JOIN tabla_historial_asistencias 
ON tabla_estudiantes.id_estudiante = tabla_historial_asistencias.id_estudiante
ORDER BY 
    tabla_estudiantes.codigo_matricula;
